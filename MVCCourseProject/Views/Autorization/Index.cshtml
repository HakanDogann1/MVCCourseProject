@model List<EntityLayer.Concrete.Admin>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>#</th>
            <th>Kullanıcı Mail</th>
            <th>Yetki</th>
            <th>Status</th>
            <th>Pasif Yap</th>
            <th>Yetksini Değiştir</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.AdminID</th>
                <td>@item.UserName</td>
                <td>@item.Role</td>
                <td>Aktif</td>
                <td><a href="#" class="btn btn-outline-danger">Pasif Yap</a></td>
                <td><a href="#" class="btn btn-outline-warning">Yetkisini Değiştir</a></td>
            </tr>
        }


    </tbody>

</table>
<br />
<a href="#" class="btn btn-outline-success">Ekle</a>



